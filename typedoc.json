{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "src/cli.ts",
    "src/migration/**/*.ts",
    "src/assessment/**/*.ts",
    "src/lib/**/*.ts",
    "src/types/**/*.ts",
    "src/schemas/**/*.ts",
    "src/dashboard/app/api/**/route.ts"
  ],
  "exclude": [
    "**/node_modules/**",
    "**/dist/**",
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/__tests__/**",
    "src/dashboard/node_modules/**"
  ],
  "out": "docs/api-reference",
  "json": "docs/api-reference/typedoc.json",
  "plugin": ["typedoc-plugin-markdown"],
  "readme": "none",
  "gitRevision": "main",
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": false,
  "includeVersion": true,
  "categorizeByGroup": true,
  "categoryOrder": [
    "CLI Commands",
    "Migration",
    "Assessment",
    "API Routes",
    "Utilities",
    "Types",
    "*"
  ],
  "sort": ["source-order"],
  "kindSortOrder": [
    "Project",
    "Module",
    "Namespace",
    "Enum",
    "Class",
    "Interface",
    "TypeAlias",
    "Constructor",
    "Property",
    "Method",
    "Accessor",
    "Function",
    "Variable"
  ]
}

